<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="@color/orange"
tools:context=".showQuestions3">

<TextView
    android:id="@+id/showChosenQuizTV23"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="20dp"
    android:layout_marginTop="16dp"
    android:layout_marginEnd="20dp"
    android:text="TextView"
    android:textColor="@color/black"
    android:textSize="16sp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

    <TextView
    android:id="@+id/answer1TV23"
    android:layout_width="0dp"
    android:layout_height="46dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:layout_marginBottom="8dp"
    android:background="@color/white"
    android:gravity="center_vertical"
    android:paddingStart="5dp"
    android:text="TextView"
    android:textColor="@color/black"
    app:layout_constraintBottom_toTopOf="@+id/answer2TV23"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toEndOf="@+id/answerBtn123" />

<TextView
    android:id="@+id/answer2TV23"
    android:layout_width="0dp"
    android:layout_height="46dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:layout_marginBottom="8dp"
    android:background="@color/white"
    android:gravity="center_vertical"
    android:paddingStart="5dp"
    android:text="TextView"
    android:textColor="@color/black"
    app:layout_constraintBottom_toTopOf="@+id/answer3TV23"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toEndOf="@+id/answerBtn223" />

<TextView
    android:id="@+id/answer3TV23"
    android:layout_width="0dp"
    android:layout_height="46dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:layout_marginBottom="2dp"
    android:background="@color/white"
    android:gravity="center_vertical"
    android:paddingStart="5dp"
    android:text="TextView"
    android:textColor="@color/black"
    app:layout_constraintBottom_toTopOf="@+id/listenUrlBtn23"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toEndOf="@+id/answerBtn323" />

<Button
    android:id="@+id/answerBtn123"
    style="@style/Widget.MaterialComponents.Button"
    android:layout_width="46dp"
    android:layout_height="46dp"
    android:layout_marginStart="16dp"
    android:layout_marginBottom="8dp"
    android:backgroundTint="@color/white"
    android:backgroundTintMode="add"
    android:insetTop="0dp"
    android:insetBottom="0dp"
    android:text="1"
    android:textColor="@color/black"
    app:cornerRadius="10dp"
    app:layout_constraintBottom_toTopOf="@+id/answerBtn223"
    app:layout_constraintStart_toStartOf="parent" />

<Button
    android:id="@+id/answerBtn223"
    style="@style/Widget.MaterialComponents.Button"
    android:layout_width="46dp"
    android:layout_height="46dp"
    android:layout_marginStart="16dp"
    android:layout_marginBottom="8dp"
    android:backgroundTint="@color/white"
    android:insetTop="0dp"
    android:insetBottom="0dp"
    android:text="X"
    android:textColor="@color/black"
    app:cornerRadius="10dp"
    app:layout_constraintBottom_toTopOf="@+id/answerBtn323"
    app:layout_constraintStart_toStartOf="parent" />

<Button
    android:id="@+id/answerBtn323"
    android:layout_width="46dp"
    android:layout_height="46dp"
    android:layout_marginStart="16dp"
    android:layout_marginBottom="51dp"
    android:backgroundTint="@color/white"
    android:insetTop="0dp"
    android:insetBottom="0dp"
    android:text="2"
    android:textColor="@color/black"
    app:cornerRadius="10dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintStart_toStartOf="parent" />

    <Button
    android:id="@+id/listenUrlBtn23"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginBottom="2dp"
    android:backgroundTint="@color/white"
    android:text="lyssna"
    android:textColor="@color/black"
    app:cornerRadius="20dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/questionTV2123"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="#F8F6F6"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/answer1TV23"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/showChosenQuizTV23" />

</androidx.constraintlayout.widget.ConstraintLayout>